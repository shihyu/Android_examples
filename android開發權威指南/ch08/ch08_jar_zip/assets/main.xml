<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent">
	<Button android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:text="用jar格式壓縮檔案"
		android:onClick="onClick_Jar_Compress" />
	<Button android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:text="解壓jar格式檔案"
		android:onClick="onClick_Jar_Uncompress" />
	<Button android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:text="用zip格式壓縮檔案"
		android:onClick="onClick_Zip_Compress" />
	<Button android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:text="解壓zip格式檔案"
		android:onClick="onClick_Zip_Uncompress" />
</LinearLayout>

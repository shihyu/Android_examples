<?xml version="1.0" encoding="utf-8"?>

<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent">
	<LinearLayout android:orientation="vertical"
		android:layout_width="fill_parent" android:layout_height="fill_parent">
		<Button android:id="@+id/btnCall" android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:text="直接撥號" />
		<Button android:id="@+id/btnDial" android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:text="將電話號傳入撥號程式" />
		<Button android:id="@+id/btnTouchDialer" android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:text="呼叫撥號程式" />
		<Button android:id="@+id/btnWeb" android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:text="瀏覽網頁" />
		<Button android:id="@+id/btnSendToEmail" android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:text="向Email客戶端傳遞Email地址" />
		<Button android:id="@+id/btnSendEmail" android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:text="發送Email" />
		<Button android:id="@+id/btnContactList" android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:text="檢視聯繫人" />
		<Button android:id="@+id/btnSettings" android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:text="系統設定" />
		<Button android:id="@+id/btnWifiSettings" android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:text="Wifi設定" />

		<Button android:id="@+id/btnAudio" android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:text="選擇音頻程式" />
	</LinearLayout>
</ScrollView>
